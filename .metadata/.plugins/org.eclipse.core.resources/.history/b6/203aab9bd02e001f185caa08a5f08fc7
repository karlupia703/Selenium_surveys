package coding;

public class ClassandObject {
	String color;
	String type;
	
	public void write() {
	System.out.println("Writing something");
	}

	public static void main(String[] args) {
		ClassandObject pen1 = new ClassandObject();
		pen1.color ="blue";
		pen1.type ="gel";
		
		pen1.write();
		
		
	}

}
